{
  "name": "jseminck-be-translation",
  "version": "1.0.0",
  "description": "API for jseminck.be",
  "main": "lib/index.js",
  "scripts": {
    "dev": "nodemon lib/index.js --exec babel-node --presets es2015,stage-2",
    "build": "rm -rf dist/ && babel lib -d dist",
    "start": "DATABASE_URL=postgres://jseminck:jseminck@localhost:10000/jseminck forever start -a -l forever.log -o out.log -e err.log dist/index.js",
    "stop": "forever stop dist/index.js",
    "lint": "eslint ./lib/ ./test/",
    "test:dev": "mocha --require test/mocha-babel",
    "test": "./node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha --report lcovonly -- --timeout 3000 --require test/mocha-babel --recursive -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "Joachim Seminck <joachim@seminck.be>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-eslint": "^6.0.2",
    "babel-istanbul": "^0.7.0",
    "babel-plugin-syntax-async-functions": "^6.1.4",
    "babel-plugin-transform-regenerator": "^6.1.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.8.0",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^1.8.1",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "aws-sdk": "^2.2.47",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.4",
    "forever": "^0.15.2",
    "jseminck-be-communication-utils": "git+https://github.com/jseminck/jseminck-be-communication-utils.git",
    "jseminck-be-pg": "git+https://github.com/jseminck/jseminck-be-pg.git",
    "jseminck-be-server": "git+https://github.com/jseminck/jseminck-be-server.git",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.6.1",
    "moment": "^2.13.0",
    "node-fetch": "^1.5.3",
    "pug": "^2.0.0-beta5"
  }
}
